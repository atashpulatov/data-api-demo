{
  "name": "react-with-webpack",
  "version": "1.0.0",
  "description": "A fixed version of the project from the `broken` branch that works as expected with webpack-dev-server.",
  "main": "index.js",
  "scripts": {
    "devNoServer": "webpack --d --watch",
    "dev": "webpack-dev-server --content-base=src/frontend/public",
    "start-dev": "webpack-dev-server --https --port 3000",
    "build": "webpack -p",
    "test": "jest",
    "test-debug": "node --nolazy --inspect-brk=9229 ./node_modules/.bin/jest -i"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./test/frontend/app/setup-tests.js",
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "jsx": "^0.9.89",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "style-loader": "^0.21.0",
    "superagent": "^3.8.3",
    "webpack": "^3.12.0"
  },
  "devDependencies": {
    "babel-polyfill": "^6.26.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-react": "^7.8.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.4",
    "jest-cli": "^22.4.4",
    "react-test-renderer": "^15.6.2",
    "webpack-dev-server": "^2.11.2"
  }
}
