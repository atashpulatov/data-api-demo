{
  "name": "mstr-office",
  "version": "1.0.0",
  "build": "",
  "private": true,
  "homepage": ".",
  "scripts": {
    "start": "HTTPS=true react-scripts start",
    "start:windows": "set HTTPS=true&&react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --silent --noStackTrace",
    "test:debug": "react-scripts test --debug",
    "test:coverage": "CI=true react-scripts test  --silent --coverage --noStackTrace --forceExit --ci --json --outputFile=./coverage/test-results.json",
    "eject": "react-scripts eject",
    "eslint": "eslint ./src --ext .js,.jsx --ignore-pattern *.test*",
    "start:desktop": "office-addin-debugging start manifest.xml desktop",
    "start:web": "office-addin-debugging start manifest.xml web",
    "stop": "office-addin-debugging stop manifest.xml",
    "dev-certs-install": "office-addin-dev-certs install",
    "sideload": "office-toolbox sideload -m manifest.xml -a excel",
    "unload": "office-toolbox remove -m manifest.xml -a excel",
    "validate": "office-toolbox validate -m manifest.xml"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "babel": {
    "presets": [
      [
        "airbnb"
      ]
    ]
  },
  "dependencies": {
    "@mstr/mstr-react-library": "^1.2.15",
    "@mstr/rc": "^0.1.30",
    "antd": "^3.22.2",
    "core-js": "^3.2.1",
    "focus-visible": "^5.0.2",
    "i18next": "^17.0.6",
    "pouchdb": "^7.1.1",
    "prop-types": "^15.7.2",
    "proxy-polyfill": "^0.3.0",
    "query-string": "^6.7.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-i18next": "^10.11.4",
    "react-redux": "^7.1.0",
    "react-scripts": "^3.1.1",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.2",
    "superagent": "^5.1.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.4.4",
    "babel-preset-airbnb": "^4.0.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "i18next-xhr-backend": "^3.1.2",
    "jest-localstorage-mock": "^2.4.0",
    "office-addin-debugging": "^3.0.10",
    "office-addin-dev-certs": "^1.2.4",
    "office-toolbox": "^0.3.0",
    "webpack-cli": "^3.1.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "ie 11",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/_tests_/**/*.{js,jsx}",
      "!src/**/*rest-service.js",
      "!src/error.jsx",
      "!src/index.js",
      "!src/popup-entrypoint.js",
      "!src/serviceWorker.js",
      "!src/office/office-api-helper.js",
      "!src/office/office-context.js",
      "!src/office/office-display-service.js",
      "!src/office/store/office-store-service.js",
      "!src/storage/session-helper.js",
      "!src/storage/pouch-db.js",
      "!src/popup/popup-controller.js",
      "!src/breadcrumbs/breadcrumbs.jsx"
    ]
  }
}
