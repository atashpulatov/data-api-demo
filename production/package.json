{
  "name": "mstr-office",
  "version": "1.0.0",
  "build": "local",
  "private": true,
  "homepage": ".",
  "scripts": {
    "start": "HTTPS=true react-scripts start",
    "start:windows": "set HTTPS=true&&react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "CI=true jest --silent --coverage --noStackTrace --forceExit --ci --json --outputFile=./coverage/test-results.json",
    "eslint": "eslint ./src --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "eslint:json": "eslint ./src --ext .js,.jsx,.ts,.tsx --max-warnings=0 --format json --output-file ./coverage/eslint.json",
    "start:desktop": "office-addin-debugging start manifest.xml desktop",
    "start:web": "office-addin-debugging start manifest.xml web",
    "stop": "office-addin-debugging stop manifest.xml",
    "dev-certs-install": "office-addin-dev-certs install",
    "validate": "office-addin-manifest validate manifest.xml"
  },
  "dependencies": {
    "@mstr/connector-components": "^6.5.17",
    "@mstr/mstr-icons": "^2.41.10",
    "@mstr/mstr-react-library": "2.0.0",
    "@mstr/rc": "^3.15.6",
    "core-js": "^3.34.0",
    "focus-visible": "^5.2.0",
    "i18next": "23.5.1",
    "lodash.throttle": "^4.1.1",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.2.2",
    "react-redux": "^9.1.0",
    "react-scripts": "^5.0.1",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^3.1.0",
    "reselect": "^5.1.0",
    "resize-observer-polyfill": "^1.5.1",
    "superagent": "^5.3.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@babel/preset-env": "^7.17.12",
    "@babel/preset-react": "^7.18.6",
    "@mstr/eslint-config-mstr": "^0.2.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/office-js": "^1.0.376",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/superagent": "^4.1.16",
    "@types/uuid": "^9.0.8",
    "eslint": "^8.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-localstorage-mock": "^2.4.18",
    "office-addin-debugging": "^5.0.14",
    "office-addin-dev-certs": "^1.12.0",
    "office-addin-manifest": "^1.12.8",
    "prettier": "^3.2.5",
    "regenerator-runtime": "^0.13.9",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3",
    "webpack-cli": "^4.9.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "ie 11",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "antd": "^3.26.20",
    "node-fetch": "2.6.8",
    "nth-check": "^2.1.1",
    "axios": "1.6.0",
    "resolve-url-loader": "5.0.0",
    "@adobe/css-tools": "4.3.2",
    "postcss": "8.4.31",
    "react-scripts": {
      "typescript": "5.3.3"
    }
  }
}
