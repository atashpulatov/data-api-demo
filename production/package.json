{
  "name": "mstr-office",
  "version": "1.0.0",
  "build": "local",
  "private": true,
  "homepage": ".",
  "scripts": {
    "start": "HTTPS=true react-scripts start",
    "start:windows": "set HTTPS=true&&react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --silent --noStackTrace",
    "test:debug": "react-scripts test --debug",
    "test:coverage": "CI=true react-scripts test  --silent --coverage --noStackTrace --forceExit --ci --json --outputFile=./coverage/test-results.json",
    "eject": "react-scripts eject",
    "eslint": "eslint ./src --ext .js,.jsx --max-warnings=0",
    "eslint:json": "eslint ./src --ext .js,.jsx --max-warnings=0 --format json --output-file ./coverage/eslint.json",
    "eslint:html": "eslint ./src --ext .js,.jsx --max-warnings=0 --format html --output-file ./coverage/eslint.html",
    "start:desktop": "office-addin-debugging start manifest.xml desktop",
    "start:web": "office-addin-debugging start manifest.xml web",
    "stop": "office-addin-debugging stop manifest.xml",
    "dev-certs-install": "office-addin-dev-certs install",
    "sideload": "office-toolbox sideload -m manifest.xml -a excel",
    "unload": "office-toolbox remove -m manifest.xml -a excel",
    "validate": "office-toolbox validate -m manifest.xml"
  },
  "dependencies": {
    "@mstr/mstr-react-library": "1.7.18",
    "@mstr/rc": "^1.5.8",
    "@types/jest": "^25.1.3",
    "antd": "^3.26.2",
    "core-js": "^3.5.0",
    "dexie": "^2.0.4",
    "dexie-observable": "^1.0.0-beta.5",
    "focus-visible": "^5.0.2",
    "i18next": "^17.3.1",
    "is-plain-object": "^3.0.0",
    "lodash.throttle": "^4.1.1",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "proxy-polyfill": "^0.3.0",
    "query-string": "^6.9.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-i18next": "^10.13.2",
    "react-redux": "^7.1.3",
    "react-scripts": "^3.4.1",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.2",
    "superagent": "^5.1.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@mstr/eslint-config-mstr": "^0.1.7",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "babel-preset-airbnb": "^4.4.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "i18next-xhr-backend": "^3.2.2",
    "jest-localstorage-mock": "^2.4.0",
    "office-addin-debugging": "^3.0.20",
    "office-addin-dev-certs": "^1.4.13",
    "office-toolbox": "^0.3.0",
    "redux-mock-store": "^1.5.4",
    "typescript": "^3.7.3",
    "webpack-cli": "^3.3.10"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "ie 11",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/_tests_/**/*.{js,jsx}",
      "!src/**/*rest-service.js",
      "!src/error.jsx",
      "!src/index.js",
      "!src/entry-point/*.*",
      "!src/serviceWorker.js",
      "!src/office/office-context.js",
      "!src/office/store/office-store-service.js",
      "!src/breadcrumbs/breadcrumbs.jsx",
      "!src/CompileStrings.js",
      "!src/cache/cache-db.js",
      "!src/dossier/embedded-dossier.jsx",
      "!src/prompts/prompts-window.jsx",
      "!src/office/office-display-service.js",
      "!src/navigation/**/*.*",
      "!src/office/format/*.*",
      "!src/office/api/*.*",
      "!src/office/table/*.*",
      "!src/office/import/*.*",
      "!src/popup/assets/*.*",
      "!src/redux-reducer/*.*"
    ]
  }
}
