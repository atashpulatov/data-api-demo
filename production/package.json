{
  "name": "mstr-office",
  "version": "1.0.0",
  "build": "local",
  "private": true,
  "homepage": ".",
  "scripts": {
    "start": "HTTPS=true react-scripts start",
    "start:windows": "set HTTPS=true&&react-scripts start",
    "build": "react-scripts build",
    "test": "jest --silent --noStackTrace",
    "test:debug": "react-scripts test --debug",
    "test:coverage": "CI=true react-scripts test  --silent --coverage --noStackTrace --forceExit --ci --json --outputFile=./coverage/test-results.json",
    "eslint": "eslint ./src --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "eslint:json": "eslint ./src --ext .js,.jsx,.ts,.tsx --max-warnings=0 --format json --output-file ./coverage/eslint.json",
    "start:desktop": "office-addin-debugging start manifest.xml desktop",
    "start:web": "office-addin-debugging start manifest.xml web",
    "stop": "office-addin-debugging stop manifest.xml",
    "dev-certs-install": "office-addin-dev-certs install",
    "validate": "office-addin-manifest validate manifest.xml"
  },
  "dependencies": {
    "@mstr/connector-components": "^4.2.4",
    "@mstr/mstr-icons": "^2.30.4",
    "@mstr/mstr-react-library": "2.0.0",
    "@mstr/rc": "npm:@mstr/rc@2.42.5",
    "@mstr/rc-3": "npm:@mstr/rc@3.8.0",
    "core-js": "^3.34.0",
    "focus-visible": "^5.2.0",
    "i18next": "23.5.1",
    "lodash.throttle": "^4.1.1",
    "moment": "^2.29.4",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.2.2",
    "react-redux": "^7.2.5",
    "react-scripts": "^5.0.1",
    "redux": "^4.1.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "resize-observer-polyfill": "^1.5.1",
    "superagent": "^5.3.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.17.12",
    "@babel/preset-react": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@mstr/eslint-config-mstr": "^0.1.7",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/superagent": "^4.1.16",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-localstorage-mock": "^2.4.18",
    "office-addin-debugging": "^5.0.14",
    "office-addin-dev-certs": "^1.12.0",
    "office-addin-manifest": "^1.12.8",
    "redux-mock-store": "^1.5.4",
    "regenerator-runtime": "^0.13.9",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3",
    "webpack-cli": "^4.9.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "ie 11",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "antd": "^3.26.20",
    "node-fetch": "2.6.8",
    "nth-check": "^2.1.1",
    "axios": "1.6.0",
    "resolve-url-loader": "5.0.0",
    "@adobe/css-tools": "4.3.2",
    "postcss": "8.4.31",
    "react-scripts": {
      "typescript": "5.3.3"
    }
  }
}
