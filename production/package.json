{
  "name": "mstr-office",
  "version": "0.1.0",
  "build": "",
  "private": true,
  "homepage": ".",
  "scripts": {
    "start": "HTTPS=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:desktop": "office-addin-debugging start manifest.xml desktop",
    "start:web": "office-addin-debugging start manifest.xml web",
    "stop": "office-addin-debugging stop manifest.xml",
    "dev-certs-install": "office-addin-dev-certs install",
    "sideload": "office-toolbox sideload -m manifest.xml -a powerpoint",
    "unload": "office-toolbox remove -m manifest.xml -a powerpoint",
    "validate": "office-toolbox validate -m manifest.xml"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "babel": {
    "presets": [
      [
        "react-app"
      ]
    ]
  },
  "dependencies": {
    "antd": "^3.19.3",
    "core-js": "^3.1.3",
    "focus-visible": "^5.0.2",
    "i18next": "^17.0.6",
    "i18next-xhr-backend": "^3.1.2",
    "proxy-polyfill": "^0.3.0",
    "query-string": "^6.7.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-i18next": "^10.11.4",
    "react-redux": "^7.1.0",
    "react-scripts": "3.0.1",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.2",
    "superagent": "^4.0.0-beta.5"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.4.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint-config-google": "^0.11.0",
    "jest-localstorage-mock": "^2.4.0",
    "office-addin-debugging": "^2.3.0",
    "office-addin-dev-certs": "^1.2.4",
    "office-toolbox": "^0.1.1",
    "redux-devtools-extension": "^2.13.5",
    "webpack-cli": "^3.1.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*rest-service.js",
      "!src/error.jsx",
      "!src/index.js",
      "!src/popup-entrypoint.js",
      "!src/serviceWorker.js",
      "!src/office/office-api-helper.js",
      "!src/office/office-context.js",
      "!src/office/office-display-service.js",
      "!src/office/store/office-store-service.js",
      "!src/storage/session-helper.js",
      "!src/popup/popup-controller.js",
      "!src/breadcrumbs/breadcrumbs.jsx",
      "!src/prompts/prompts-rest-service.jsx"
    ]
  }
}
