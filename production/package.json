{
  "name": "mstr-office",
  "version": "1.0.0",
  "build": "local",
  "private": true,
  "homepage": ".",
  "scripts": {
    "start": "HTTPS=true react-scripts start",
    "start:windows": "set HTTPS=true&&react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --silent --noStackTrace",
    "test:debug": "react-scripts test --debug",
    "test:coverage": "CI=true react-scripts test  --silent --coverage --noStackTrace --forceExit --ci --json --outputFile=./coverage/test-results.json",
    "eslint": "eslint ./src --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "eslint:json": "eslint ./src --ext .js,.jsx,.ts,.tsx --max-warnings=0 --format json --output-file ./coverage/eslint.json",
    "start:desktop": "office-addin-debugging start manifest.xml desktop",
    "start:web": "office-addin-debugging start manifest.xml web",
    "stop": "office-addin-debugging stop manifest.xml",
    "dev-certs-install": "office-addin-dev-certs install",
    "validate": "office-addin-manifest validate manifest.xml"
  },
  "dependencies": {
    "@mstr/connector-components": "^4.0.1",
    "@mstr/mstr-icons": "^2.30.4",
    "@mstr/mstr-react-library": "2.0.0",
    "@mstr/rc": "^2.22.3",
    "core-js": "^3.34.0",
    "focus-visible": "^5.2.0",
    "i18next": "^21.9.0",
    "lodash.throttle": "^4.1.1",
    "moment": "^2.29.4",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.16.9",
    "react-redux": "^7.2.5",
    "react-scripts": "^5.0.1",
    "redux": "^4.1.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "resize-observer-polyfill": "^1.5.1",
    "superagent": "^5.3.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.17.12",
    "@babel/preset-react": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@mstr/eslint-config-mstr": "^0.1.7",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/superagent": "^4.1.16",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "jest-localstorage-mock": "^2.4.18",
    "office-addin-debugging": "^5.0.14",
    "office-addin-dev-certs": "^1.12.0",
    "office-addin-manifest": "^1.12.8",
    "redux-mock-store": "^1.5.4",
    "regenerator-runtime": "^0.13.9",
    "typescript": "^5.3.3",
    "webpack-cli": "^4.9.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "ie 11",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "resetMocks": false,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/_tests_/**/*.{js,jsx}",
      "!src/**/*rest-service.js",
      "!src/error.jsx",
      "!src/index.js",
      "!src/entry-point/*.*",
      "!src/serviceWorker.js",
      "!src/office/office-context.js",
      "!src/office/store/office-store-service.js",
      "!src/breadcrumbs/breadcrumbs.jsx",
      "!src/dossier/embedded-dossier.jsx",
      "!src/prompts/prompts-window.jsx",
      "!src/office/office-display-service.js",
      "!src/navigation/**/*.*",
      "!src/office/format/*.*",
      "!src/office/api/*.*",
      "!src/office/import/*.*",
      "!src/popup/assets/*.*",
      "!src/redux-reducer/*.*"
    ],
    "moduleNameMapper": {
      "^antd/es/(.*)$": "antd/lib/$1",
      "^lodash-es$": "lodash"
    }
  },
  "overrides": {
    "antd": "^3.26.20",
    "node-fetch": "2.6.8",
    "nth-check": "^2.1.1",
    "axios": "1.6.0",
    "resolve-url-loader": "5.0.0",
    "@adobe/css-tools": "4.3.2",
    "postcss": "8.4.31",
    "react-scripts": {
        "typescript": "5.3.3"
    }
  }
}
