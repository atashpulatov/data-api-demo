{
  "extends": [
    "@mstr/eslint-config-mstr"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "globals": {
    "Excel": true,
    "microstrategy": true,
    "Office": true,
    "OfficeExtension": true
  },
  "rules": {
    "semi": ["warn", "always"],
    "jsx-quotes": ["warn", "prefer-double"]
  }
}