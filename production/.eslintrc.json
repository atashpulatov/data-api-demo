{
  "extends": [
    "react-app",
    "airbnb"
  ],
  "parserOptions": {
    "ecmaVersion": 9
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "globals": {
    "Excel": true,
    "microstrategy": true,
    "Office": true,
    "OfficeExtension": true
  },
  "rules": {
    "class-methods-use-this": "warn",
    "jsx-a11y/click-events-have-key-events": "warn",
    "no-console": "off",
    "react/jsx-props-no-spreading": "warn",
    "react/prop-types": "off"
  }
}