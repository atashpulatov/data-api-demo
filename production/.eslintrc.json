{
    "extends": [
        "google",
        "react-app"
    ],
    "parserOptions": {
        "ecmaVersion": 8
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "mocha": true
    },
    "globals": {
        "Office": true
    },
    "rules": {
        "max-len": [
            "warn",
            250
        ],
        "no-multi-str": "off",
        "require-jsdoc": "off",
        "no-invalid-this": "off"
    }
}