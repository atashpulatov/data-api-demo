{
  "compilerOptions": {
    "resolveJsonModule": true,
    "outDir": "./dist/",
    "sourceMap": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "allowJs": true,
    "noImplicitAny": true,
    "strictBindCallApply": true,
    "alwaysStrict": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
